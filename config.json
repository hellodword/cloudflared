{
  "log": {
    "loglevel": "none"
  },
  "inbounds": [
    {
      "tag": "vmess",
      "protocol": "vmess",
      "listen": "127.0.0.1",
      "port": 8001,
      "settings": {
        "clients": [
          {
            "id": "CONFIG_UUID",
            "alterId": 0,
            "security": "auto"
          }
        ]
      },
      "streamSettings": {
        "network": "ws",
        "wsSettings": {
          "path": "/test"
        }
      },
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "http",
          "tls"
        ]
      }
    },
    {
      "tag": "vmess2",
      "protocol": "vmess",
      "listen": "127.0.0.1",
      "port": 8002,
      "settings": {
        "clients": [
          {
            "id": "CONFIG_UUID",
            "alterId": 0,
            "security": "auto"
          }
        ]
      },
      "streamSettings": {
        "network": "ws",
        "wsSettings": {
          "path": "/test2"
        }
      },
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "http",
          "tls"
        ]
      }
    }
  ],
  "outbounds": [
    {
      "tag": "direct",
      "protocol": "freedom",
      "settings": {}
    },
    {
      "tag": "direct2",
      "protocol": "freedom",
      "sendThrough": "0.0.0.0",
      "settings": {
        "domainStrategy": "UseIP"
      }
    }
  ],
  "routing": {
    "strategy": "rules",
    "settings": {
      "rules": [
        {
          "type": "field",
          "inboundTag": [
            "vmess"
          ],
          "outboundTag": "direct"
        },
        {
          "type": "field",
          "inboundTag": [
            "vmess2"
          ],
          "outboundTag": "direct2"
        }
      ]
    }
  }
}